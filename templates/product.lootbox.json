e,"gap":48,"color_scheme":"","padding-block-start":0,"padding-block-end":0}},"custom_liquid_fggcBW":{"type":"custom-liquid","name":"t:names.custom_liquid","settings":{"custom_liquid":"<!-- 1. DATA CONFIGURATION -->\n<script>\n  var currentVariantId = \"{{ product.selected_or_first_available_variant.id }}\";\n  \/\/ Pull data from Metafields\n  var lootTable = [\n    {% if product.metafields.custom.lootbox.value %}\n      {% for item in product.metafields.custom.lootbox.value %}\n      {\n        name: \"{{ item.name }}\",\n        image: \"{{ item.image | image_url: width: 600 }}\",\n        chance: {{ item.chance | default: 0 }}\n      },\n      {% endfor %}\n    {% endif %}\n  ];\n<\/script>\n\n<!-- 2. TRIGGER BUTTON (Displayed on Product Page) -->\n<div class=\"lootbox-trigger-wrapper\">\n  <button id=\"btn-start-gacha\" onclick=\"playGacha()\">\n    <div class=\"btn-content\">\n      <span class=\"icon\">üé∞<\/span>\n      <span class=\"text\">UNLOCK NOW<\/span>\n    <\/div>\n    <span class=\"sub-text\">{{ product.price | money }} \/ Attempt<\/span>\n  <\/button>\n  <p class=\"note\">‚ú® You will be redirected to checkout immediately after winning.<\/p>\n<\/div>\n\n<!-- 3. GACHA OVERLAY -->\n<div id=\"gacha-overlay\" style=\"display:none;\">\n  <div class=\"overlay-bg\"><\/div>\n  <div id=\"flash-screen\"><\/div>\n\n  <!-- CENTER CONTAINER -->\n  <div class=\"gacha-center-stage\">\n\n    <!-- PHASE 1: SHAKING BOX -->\n    <div id=\"phase-shake\" class=\"flex-center-content\">\n      <img src=\"{{ product.featured_image | image_url: width: 500 }}\" class=\"box-image\" alt=\"Mystery Box\">\n      <h2 class=\"status-text\">SUMMONING...<\/h2>\n    <\/div>\n\n    <!-- PHASE 2: RESULT REVEAL -->\n    <div id=\"phase-result\" class=\"flex-center-content\" style=\"display:none;\">\n      \n      <!-- Halo Glow Effect -->\n      <div class=\"halo-glow\"><\/div>\n      \n      <!-- Prize Image -->\n      <div class=\"prize-wrapper\">\n        <img id=\"prize-img\" src=\"\" alt=\"Prize\">\n      <\/div>\n      \n      <!-- Prize Info -->\n      <div class=\"prize-info\">\n        <div class=\"label\">YOU OBTAINED<\/div>\n        <h2 id=\"prize-name\">ITEM NAME<\/h2>\n        <div class=\"badge\" id=\"prize-chance\">Rate: 1%<\/div>\n      <\/div>\n\n      <!-- Success\/Redirect Message -->\n      <div class=\"cart-msg\" id=\"redirect-msg\">\n        <span class=\"check-icon\">‚è≥<\/span> Processing Checkout...\n      <\/div>\n\n      <!-- ACTION BUTTONS (Hidden initially, only strictly needed if redirect fails) -->\n      <div class=\"action-buttons\" style=\"pointer-events: none; opacity: 0.5;\">\n        <button class=\"btn-action btn-back\">\n           PLEASE WAIT...\n        <\/button>\n      <\/div>\n\n    <\/div>\n\n  <\/div>\n<\/div>\n\n<!-- 4. CSS STYLING (Gi·ªØ nguy√™n style c≈©) -->\n<style>\n  \/* --- LAYOUT UTILS --- *\/\n  .flex-center-content {\n    display: flex; flex-direction: column; align-items: center; justify-content: center; width: 100%;\n  }\n  .box-image, #prize-img { \n    display: block; margin: 0 auto !important; max-width: 100%; object-fit: contain; \n  }\n\n  \/* BUTTON STYLES (Trigger) *\/\n  .lootbox-trigger-wrapper { margin: 20px 0; text-align: center; width: 100%; }\n  #btn-start-gacha {\n    width: 100%; background: linear-gradient(135deg, #ff0055 0%, #ff4400 100%);\n    border: none; padding: 15px; border-radius: 10px; color: white; cursor: pointer;\n    box-shadow: 0 4px 15px rgba(255, 0, 85, 0.4); transition: transform 0.2s;\n  }\n  #btn-start-gacha:hover { transform: scale(1.02); }\n  .btn-content { display: flex; align-items: center; justify-content: center; gap: 8px; font-size: 20px; font-weight: 800; }\n  .sub-text { font-size: 13px; opacity: 0.9; margin-top: 4px; display: block; }\n  .note { font-size: 12px; color: #888; margin-top: 8px; font-style: italic; }\n\n  \/* OVERLAY STYLES *\/\n  #gacha-overlay {\n    position: fixed; top: 0; left: 0; width: 100vw; height: 100vh;\n    z-index: 2147483647; \n    display: flex; align-items: center; justify-content: center;\n    flex-direction: column;\n  }\n  .overlay-bg { position: absolute; width: 100%; height: 100%; background: rgba(0,0,0,0.95); z-index: 1; }\n\n  \/* FLASH EFFECT *\/\n  #flash-screen { position: absolute; top: 0; left: 0; width: 100%; height: 100%; background: white; z-index: 5; opacity: 0; pointer-events: none; }\n  .do-flash { animation: flash 0.6s ease-out; }\n  @keyframes flash { 0% { opacity: 1; } 100% { opacity: 0; } }\n\n  \/* CENTER STAGE *\/\n  .gacha-center-stage { position: relative; z-index: 10; text-align: center; width: 100%; max-width: 600px; padding: 20px; }\n\n  \/* ANIMATIONS *\/\n  .box-image { width: 300px; height: 300px; filter: drop-shadow(0 10px 20px rgba(0,0,0,0.5)); }\n  .shake-anim { animation: shake 0.5s infinite; }\n  @keyframes shake { 0%{transform:rotate(0deg)} 25%{transform:rotate(5deg)} 50%{transform:rotate(0deg)} 75%{transform:rotate(-5deg)} 100%{transform:rotate(0deg)} }\n  \n  .status-text { font-size: 30px; color: white; letter-spacing: 2px; font-weight: 300; margin-top: 20px; text-shadow: 0 2px 10px black; }\n\n  \/* HALO GLOW *\/\n  .halo-glow {\n    position: absolute; top: 50%; left: 50%; transform: translate(-50%, -50%);\n    width: 600px; height: 600px;\n    background: radial-gradient(circle, rgba(255, 215, 0, 0.4) 0%, rgba(0,0,0,0) 70%);\n    z-index: -1; animation: breathe 3s infinite alternate;\n  }\n  @keyframes breathe { from { opacity: 0.5; transform: translate(-50%, -50%) scale(1); } to { opacity: 0.8; transform: translate(-50%, -50%) scale(1.1); } }\n\n  #prize-img { width: 320px; height: 320px; filter: drop-shadow(0 0 15px gold); animation: zoomIn 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275); }\n  @keyframes zoomIn { from { transform: scale(0); opacity: 0; } to { transform: scale(1); opacity: 1; } }\n\n  .prize-info { margin-top: 15px; position: relative; z-index: 2; width: 100%; }\n  .label { color: #aaa; font-size: 14px; letter-spacing: 2px; margin-bottom: 5px; text-transform: uppercase; }\n  #prize-name { font-size: 36px; color: #ffcc00; margin: 0; font-weight: 900; text-transform: uppercase; text-shadow: 0 5px 15px rgba(0,0,0,0.8); }\n  .badge { display: inline-block; background: #222; border: 1px solid #444; color: #fff; padding: 4px 12px; border-radius: 20px; margin-top: 10px; font-size: 14px; }\n  \n  \/* S·ª≠a style cart-msg ƒë·ªÉ lu√¥n hi·ªán khi redirect *\/\n  .cart-msg { color: #4cd137; font-weight: bold; margin-top: 20px; font-size: 16px; display: flex; align-items: center; justify-content: center; gap: 8px; }\n  \n  .action-buttons {\n    margin-top: 25px;\n    display: flex; justify-content: center; gap: 15px; width: 100%;\n  }\n  .btn-action {\n    padding: 12px 25px; border-radius: 50px; font-size: 14px; font-weight: 800;\n    cursor: pointer; transition: all 0.2s; text-transform: uppercase; min-width: 140px;\n    display: flex; align-items: center; justify-content: center; gap: 8px;\n    background: rgba(255,255,255,0.1); border: 1px solid white; color: white;\n  }\n<\/style>\n\n<!-- 5. JAVASCRIPT LOGIC (ƒê√£ s·ª≠a logic Checkout) -->\n<script>\n  document.addEventListener(\"DOMContentLoaded\", function() {\n    const overlay = document.getElementById('gacha-overlay');\n    if(overlay) { document.body.appendChild(overlay); }\n  });\n\n  function playGacha() {\n    if (lootTable.length === 0) return alert(\"Data missing in Metafield custom.lootbox\");\n\n    const overlay = document.getElementById('gacha-overlay');\n    const boxImg = document.querySelector('.box-image');\n    const phaseShake = document.getElementById('phase-shake');\n    const phaseResult = document.getElementById('phase-result');\n    const flash = document.getElementById('flash-screen');\n\n    \/\/ Reset UI\n    overlay.style.display = 'flex';\n    phaseShake.style.display = 'flex';\n    phaseResult.style.display = 'none';\n    \n    \/\/ Start Animation\n    boxImg.classList.add('shake-anim');\n    flash.classList.remove('do-flash');\n\n    \/\/ Wait 2.5s for shake effect\n    setTimeout(() => {\n      \/\/ Trigger Flash\n      flash.classList.add('do-flash');\n\n      \/\/ Swap Screens\n      setTimeout(() => {\n        boxImg.classList.remove('shake-anim');\n        phaseShake.style.display = 'none';\n        phaseResult.style.display = 'flex'; \n        \n        \/\/ Random Logic\n        const reward = getWeightedRandom(lootTable);\n        document.getElementById('prize-img').src = reward.image;\n        document.getElementById('prize-name').innerText = reward.name;\n        document.getElementById('prize-chance').innerText = \"Rate: \" + reward.chance + \"%\";\n\n        \/\/ --- LOGIC M·ªöI: Th√™m v√†o gi·ªè v√† CHUY·ªÇN H∆Ø·ªöNG ---\n        addToCartAndRedirect(currentVariantId, reward);\n        \n      }, 100);\n    }, 2500);\n  }\n\n  function closeGacha() {\n    document.getElementById('gacha-overlay').style.display = 'none';\n  }\n\n  function getWeightedRandom(items) {\n    let total = items.reduce((sum, item) => sum + item.chance, 0);\n    let random = Math.random() * total;\n    let sum = 0;\n    for (let i = 0; i < items.length; i++) {\n      sum += items[i].chance;\n      if (random <= sum) return items[i];\n    }\n    return items[0];\n  }\n\n  \/\/ --- H√ÄM X·ª¨ L√ù THANH TO√ÅN M·ªöI ---\n  function addToCartAndRedirect(id, item) {\n    \/\/ Th√¥ng b√°o cho kh√°ch bi·∫øt ƒëang chuy·ªÉn h∆∞·ªõng\n    const msgDiv = document.getElementById('redirect-msg');\n    msgDiv.innerHTML = '<span class=\"check-icon\">üí≥<\/span> Redirecting to checkout...';\n    msgDiv.style.opacity = '1';\n\n    fetch(window.Shopify.routes.root + 'cart\/add.js', {\n      method: 'POST',\n      headers: { 'Content-Type': 'application\/json' },\n      body: JSON.stringify({\n        'items': [{\n          'id': id,\n          'quantity': 1,\n          'properties': {\n            'üèÜ Reward': item.name,\n            'üé≤ Rate': item.chance + '%',\n            '_image': item.image\n          }\n        }]\n      })\n    })\n    .then(response => {\n      if (response.ok) {\n        \/\/ --- CHUY·ªÇN H∆Ø·ªöNG SANG CHECKOUT ---\n        window.location.href = \"\/checkout\";\n      } else {\n        console.error(\"Error adding to cart\");\n        msgDiv.innerText = \"Error! Please try again.\";\n      }\n    })\n    .catch(e => {\n      console.error(e);\n      msgDiv.innerText = \"Connection failed.\";\n    });\n  }\n<\/script>","color_scheme":"","section_width":"full-width","padding-block-start":0,"padding-block-end":0}},"product_recommendations_qggXJq":{"type":"product-recommendations","blocks":{"text_cbcgyb":{"type":"text","name":"t:names.header","settings":{"text":"<h3>You may also like <\/h3>","width":"fit-content","max_width":"normal","alignment":"left","type_preset":"h4","font":"var(--font-primary--family)","font_size":"","line_height":"normal","letter_spacing":"normal","case":"none","wrap":"pretty","color":"","background":false,"background_color":"#00000026","corner_radius":0,"padding-block-start":0,"padding-block-end":0,"padding-inline-start":0,"padding-inline-end":0},"blocks":{}},"static-product-card":{"type":"_product-card","name":"t:names.product_card","static":true,"settings":{"product_card_gap":8,"inherit_color_scheme":true,"color_scheme":"","border":"none","border_width":1,"border_opacity":100,"border_radius":0,"padding-block-start":0,"padding-block-end":8,"padding-inline-start":0,"padding-inline-end":0},"blocks":{"product_card_gallery_DNizbJ":{"type":"_product-card-gallery","name":"t:names.product_card_media","settings":{"image_ratio":"adapt","border":"none","border_width":1,"border_opacity":100,"border_radius":0,"padding-block-start":0,"padding-block-end":0,"padding-inline-start":0,"padding-inline-end":0},"blocks":{}},"product_title_M7MJkb":{"type":"product-title","name":"t:names.product_title","settings":{"width":"100%","max_width":"normal","alignment":"left","type_preset":"rte","font":"var(--font-body--family)","font